/*app*/
@charset "utf-8";
*{margin:0;padding:0;outline:0 none;border:0 none;-webkit-text-size-adjust:none; font-family: 'pingFangSC-Medium'; }
img{vertical-align:middle;-ms-interpolation-mode:bicubic;max-width: 100%}
p{margin: 0px;}
input::-ms-clear{display:none}
input::-ms-reveal{display:none}
button,input,select,textarea{font-size:100%;vertical-align:middle;outline:0;font-family:"pingFangSC-Medium"}
button,input[type=button],input[type=text],input[type=reset],input[type=submit]{-webkit-appearance:none;display:initial;cursor:pointer;-webkit-appearance:button;-moz-appearance:button}
input:focus:-moz-placeholder,input:focus::-webkit-input-placeholder{color:transparent}
button::-moz-focus-inner,input::-moz-focus-inner{padding:0;border:0}
.btn.active, .btn:active,.btn.focus, .btn:focus, .btn:hover {background-image: none;-webkit-box-shadow:none; box-shadow: none;color: #FFFFff;
}

/*首页*/
.container{
    width: 100%;
}
a{
    text-decoration: none;
}
 /*头部*/
header{
    height: 50px;
    line-height: 50px;
    font-size: 18px;
    color: black;
    background-color: white;
}
header .header_left{
    float: left;
    width: 20%;
    text-align: left;
}
header .header_left img{
    width: 30px;
    height: 30px;
}
header .header_middle{
    float: left;
    width: 60%;
    text-align: center;
    font-size:14px;
    color: black;
}
header .header_right{
    float: right;
    width: 10%;
    text-align: right;
    padding-right: 15px;
    font-size: 12px;
}
header .header_right a{
    color: white;
}


/*搜索*/
.search{
    display: flex;
    height: 38px;
    line-height: 38px;
    /* padding: 0 15px; */
    margin-bottom: 5px;
    padding-left: 10px;
}
.search .search_a{
    width: 94%;
    /*background-color: cyan;*/
}
.search .search_b{
    width: 8%;
    float: right;
    text-align: right;
}

.search .search_a a img{
    position: absolute;
    left: 30%;
    top: 53px;
    width: 18px;
    height: 18px;
}
.search input{
    text-align: center;
    width: 100%;
    height: 30px;
    margin-top: 4px;
    background-color: rgba(246, 246, 246, 1);
    border-radius: 43px 43px 43px 43px;
    font-size: 12px !important;
    color: #333333;
}
.s{
    margin-top: 8px;
    width: 35px;
    height: 30px;
}
.biaoti{
    font-size: 13px;
    color: black;
}
.wenzi{
    font-size: 12px;
    color: #8c8c8c !important;
}
/*消息和点关系*/
.xiaoxi{
    position: absolute;
    width: 30px;
    height: 30px;
    padding-left: 0px;
    padding-top: 10px;
}

.dot{
    width: 5px;
    height: 5px;
    position: relative;
    top: -10px;
    left: 19px;

}
.icon-logo{
    width: 12px;
    height: 12px;
}
/*产品*/
.product{
    width: 100%;
    padding: 10px 0px;
    display: flex;
    background: white;
}
.product .item{
    width: 25%;
    text-align: center;
    height:65px;
}

.product .item img{
    width: 40px;
    height: 40px;
    margin-bottom: 5px;
}
.product .item a div{
    font-size: 10px;
    color: #505050;;
}
/*主体内容*/
article{

}
/*推荐VIP商家*/
.artcle_1{
    background-color: #f6f6f6;
    height: 175px;
}
.artcle_1 .top{
    text-align: center;
    overflow: hidden;
    /*padding: 5px 0;*/
    height: 40px;
    line-height: 40px;
}
.mains {
    width: 100%;
    overflow-x: auto;
    display: flex;
}
.main{
    /*margin-right: 10px;*/
    width: 100%;
    margin-left: 10px;
}
.mains::-webkit-scrollbar {
    display: none;
}
.main .list{
    width: 75px;
    text-align: center;
    background-color: white;
    padding: 15px 5px 10px 5px;
}
.main .list img{
    width: 50px;
    height: 50px;
    border-radius: 50%;
}
.main .list p{
    font-size: 14px;
}
.yichu{
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}
/*供求信息*/
.gongqiu{
    height: 40px;
    line-height:40px;
    width: 100%;
    padding: 0px 10px;
    box-sizing: border-box;
    border-bottom: 1px solid #dedede;
    background: white;
}
.gongqiu span:first-child{
    float: left;color: #333
}
.gongqiu span:last-child{
    float: right;
    color: #999;
    font-size: 15px
}
.more{
    float: right;
    margin-right: 5px;
}
.sections{
    /*border-top: 10px solid #F6F6F6;*/
    width: 100%;
    padding-bottom: 10px;
}
.section{
    width: 100%;
    overflow-x: auto;
    display: flex;
}
.section_top{
    font-size: 12px;
    padding: 5px 15px 0px 15px;
}
.section_top button{
    background-color: #ff4c82;
    color: white;
    height: 16px;
    width: 16px;
    font-size: 11px;
    border-radius: 4px;
    margin-right: 5px;
    line-height: 16px;
}
.paddingl_2{
    height: 40px;
    line-height: 40px;
}
.section_buttom{
    padding: 5px 15px 5px 0px;
    padding-left: 14px;
    font-size: 11px;
    border-bottom: 1px solid #e6e6e6;
    background: white;
}
.section_buttom p{
    color: #999999;
    font-size: 11px;
    line-height: 1.4em;
}
.section_item{
    font-size: 11px;
    margin-left: 15px;
}
/*父容器去掉滚动条*/
.section::-webkit-scrollbar {
    display: none;
}
.section_item .rongqi{
    width: 90px;
    height: 180px;
    padding-top: 10px;
    border-radius: 8px 8px 8px 8px;
}

.section_item .rongqi p,.section_buttom p{
    /*padding-top: 5px;*/
    line-height: 1.6em;
    /*color: #999999;*/
    margin: 3px 0px;
}

/*尽在咫尺*/
.distance{
    background-color: white;
    height: auto;
    padding-bottom:5px;
}
.distance p{
    padding: 5px 10px;
    line-height: 1.6em;
    font-size: 13px;
    color: #999999;
}
.distance_top{
    border-bottom: 1px solid #F6F6F6;
    padding: 10px 10px 10px 0px;
    margin-left: 10px;
    font-size: 13px;
}
.distance_top img{
    width: 25px;
    height: 25px;
    border-radius: 50%;
    padding-right: 3px;
}
.distance_top p{
    color: black;
    font-size: 18px
}
.distance_top span{
    color: #999999;
    font-size: 11px;
    padding-left: 5px;
}
.section_item .rongqi2{
    width: 110px;
    height: 110px;
    border-radius: 8px 8px 8px 8px;
    padding-top: 5px;
}
.section_item .rongqi2 img{
    height:110px;
    width:110px
}
.distance_buttom{
    padding:0px 10px;
    display: flex;
    justify-content:space-between;
    background-color: white;

}
.distance_buttom_1 span{
    color: #0f90fe;
    font-size: 12px;
}
.distance_buttom_2 span{
    color: #999999;
    font-size: 10px;
}
.distance_buttom_2 img{
    width: 20px;
    height: 20px;
}


/*尾部*/
footer{
    background-color: white;
    display: flex;
    flex-direction: row;
    z-index: 3;
    width: 100%;
    position: fixed;
    bottom: 0px;
    height: 50px;
    border-top: 1px solid  #e6e6e6;
}
footer .footer_item{
    width: 33.333%;
    height: 100%;
    padding-bottom: 5px;
    text-align: center;
    float: left;
}
.footer_item img{
    width: 23px;
    height: 25px;
    margin:5px 0;
}
.footer_item div{
    font-size: 11px;
    line-height: 10px;
}
footer .footer_item a{
    text-decoration: none;
    color: #999999 !important;
    font-size: 10px;
}
.active{
    color: #ff4c82;
}
